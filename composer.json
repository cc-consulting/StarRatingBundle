    "keywords": ["symfony", "fyneworks", "star-rating", "form", "twig"],
    "homepage": "http://github.com/cc-consulting/StarRatingBundle",
    "license": ["MIT"],
    "authors": [
        {
            "name": "Evan Owens",
            "email": "evan.owens@ccconsulting.jp"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "fyneworks/star-rating": "dev-master",
        "symfony/form": "~2.2"
    },
    "autoload": {
        "psr-0": { "CCC\\StarRatingBundle": "" }
    },
    "target-dir": "CCC/StarRatingBundle",
    "minimum-stability": "dev"
}
